{{<base}}
    {{$body}}
    <h1>Parks</h1>
        <table class="table table-bordered table-hover">
        <thead class="table-light">
            <tr>
                <th>Name</th>
                <th>Latitude</th>
                <th>Longitude</th>
            </tr>
        </thead>
        <tbody>
            {{#parks}}
            <tr>
                <td>
                    {{wrappedValue(_name)}}
                </td>
                <td>
                    {{0(_coordinates)}}
                </td>
                <td>
                    {{1(_coordinates)}}
                </td>
            </tr>
            {{/parks}}
    </tbody>
</table>
     {{/body}}
{{/base}}
